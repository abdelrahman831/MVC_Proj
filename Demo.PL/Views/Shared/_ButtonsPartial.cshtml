@model string
<td>
    <a class="btn btn-primary" asp-action="Details" asp-route-Id="@Model">
        <i class="fas fa-eye"></i>
    </a>



    <a class="btn btn-success" asp-action="Edit" asp-route-Id="@Model">
        <i class="fas fa-edit"></i>
    </a>



    <button type="button" class="btn btn-danger" data-bs-toggle="modal" asp-route-Id="@Model" data-bs-target="#deleteModal-@Model">

        <i class="fas fa-trash"></i>
    </button>

</td>