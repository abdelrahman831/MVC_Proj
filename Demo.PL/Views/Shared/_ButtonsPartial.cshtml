        <i class="fas fa-trash"></i> Delete
    </a> *@
    <button type="button" class="btn btn-danger" data-bs-toggle="modal" asp-route-Id="@Model" data-bs-target="#deleteModal-@Model">

        <i class="fas fa-trash"></i> Delete
    </button>

</td>